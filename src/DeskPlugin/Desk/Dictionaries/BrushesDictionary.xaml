<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="GraySolidColorBrush" Color="#737373"/>

    <LinearGradientBrush x:Key="GrayLinearGradientBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#CFCFCF" Offset="0"/>
        <GradientStop Color="#808080" Offset="0.5"/>
        <GradientStop Color="#4D4D4D" Offset="1"/>
    </LinearGradientBrush>

</ResourceDictionary>